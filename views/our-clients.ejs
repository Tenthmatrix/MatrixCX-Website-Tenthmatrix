<% include include/main-header %>
<style>
.blog-item h1 {
	margin-top:0px!important;
}
</style>
</head>
<body>
 	<!--header end-->
	<% include include/header %>
	<!--header end-->
	
	<style>
	.clientlogo1{
	border:1px solid #b0b0b0;
 width: 217px;
height: 154px;
	text-align:center;
	padding:10px;
	display:inline-block;
	vertical-align:top;
	border-radius: 3px;
	
  position: relative;
	
}




.clientlogo1:hover{
	cursor:pointer;
-webkit-box-shadow: 0px 0px 7px 0px rgba(50, 50, 50, 0.36);
-moz-box-shadow:    0px 0px 7px 0px rgba(50, 50, 50, 0.36);
box-shadow:         0px 0px 7px 0px rgba(50, 50, 50, 0.36);
border:1px solid #00a3d3;
}
.displayed {
  max-height: 100%;
  max-width: 100%;
  width: auto;
  height: auto;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}

.clienttxt{
	text-align:center!important;
	padding-top:5px;
	
	
}

.clientbox{
width: 265px;
float: left;
height: 179px;
text-align: center;
margin-bottom: 20px;
}
</style>
 
	<div class="breadcrumbs">
      <div class="container">
        <div class="row">
          <div class="col-lg-4 col-sm-4">
           <h1 class="mainheading"><span>Our Clients</span></h1>
          </div>
          <div class="col-lg-8 col-sm-8">
            <ol class="breadcrumb pull-right">
              <li>
                <a href="/">
                  Home
                </a>
              </li>
              <li class="active">
               Our Clients
              </li>
            </ol>
          </div>
        </div>
      </div>
    </div>
	
	<div class="container">
        <div class="row  mar-b-30">
        	<div id="portfoliolist">
           		<div class="col-md-12">
            		<% if (typeof resultData.Objects !== 'undefined' && resultData.Objects !== null ) {	
            			if(resultData.Objects.length>=1){
            				var objectItems=resultData.Objects;
            				objectItems.sort(dynamicSort("-order_by"));
            				for(var i=0; i<objectItems.length; i++){
            					if(objectItems[i].status=="1" || objectItems[i].status==1){	%>
            				<%- objectItems[i].content %>
            		<%			}
            				}
            			}else{ %>
            				<div class="alert alert-danger">No clients information is provided!</div>
            			<%
            			}
            		}else{ %>
            			<div class="alert alert-danger">No clients information is provided!</div>
            		<% } %>
            	</div>
			</div>
		</div>
    </div>
	
   	<!--footer start-->
    <% include include/footer %>
    <!--footer end-->
 	<script src="js/jquery.magnific-popup.js"></script>
	<script src="js/mixitup.js"></script>
	
	
	<!--common script for all pages-->

	<script type="text/javascript">
    $('.image-caption a').tooltip();
    $(function () {

        var filterList = {

            init: function () {

                // MixItUp plugin
                // http://mixitup.io
                $('#portfoliolist').mixitup({
                    targetSelector: '.portfolio',
                    filterSelector: '.filter',
                    effects: ['fade'],
                    easing: 'snap',
                    // call the hover effect
                    onMixEnd: filterList.hoverEffect()
                });

            },

            hoverEffect: function () {
                $("[rel='tooltip']").tooltip();
                // Simple parallax effect
                $('#portfoliolist .portfolio .portfolio-hover').hover(
        function(){
            $(this).find('.image-caption').slideDown(250); //.fadeIn(250)
        },
        function(){
            $(this).find('.image-caption').slideUp(250); //.fadeOut(205)
        }
    );



            }

        };

        // Run the show!
        filterList.init();


    });

    $( document ).ready(function() {

        $('.magnefig').each(function(){
            $(this).magnificPopup({
                type:'image',
                removalDelay: 300,
                mainClass: 'mfp-fade'
            })
        });
    });
    </script>
  </body>
</html>