								 	<% 	}else if(bookmarkItems[count].type=="html"){  %>

								 			<%- bookmarkItems[count].content %>

								 	<%	}

								 	}	 %>

								 </ul>

							</li>

					<%		}

						}

					} 

				}	

			} %>

			</ul>

		</nav>

	</div>

</div>















